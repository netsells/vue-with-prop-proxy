{
  "name": "@netsells/vue-with-prop-proxy",
  "version": "0.1.1",
  "description": "Proxy a prop in Vue to make it easier to set it if using v-model or the sync modifier",
  "main": "dist/index.js",
  "scripts": {
    "codecov": "codecov",
    "test": "jest",
    "test:mutate": "stryker run",
    "lint": "eslint --max-warnings 0 src spec",
    "docs": "jsdoc src",
    "build": "babel src -d dist",
    "prepublishOnly": "yarn build"
  },
  "author": "Sam Boylett <sam.boylett@netsells.co.uk>",
  "license": "ISC",
  "devDependencies": {
    "@netsells/eslint-config": "^1.8.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.1.0",
    "eslint": "^4.10.0",
    "eslint-plugin-jest": "^22.1.2",
    "express": "^4.16.4",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "regenerator-runtime": "^0.13.1",
    "start-server-and-test": "^1.7.11",
    "stryker": "^0.33.1",
    "stryker-api": "^0.22.0",
    "stryker-babel-transpiler": "^0.7.10",
    "stryker-html-reporter": "^0.16.9",
    "stryker-javascript-mutator": "^0.12.1",
    "stryker-jest-runner": "^1.2.9",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/netsells/vuex-rest-api-cache.git"
  },
  "jest": {
    "moduleNameMapper": {
      "^~/(.+)$": "<rootDir>/src/$1"
    },
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
